retrieve LIBPATH->lib
retrieve INCLUDEPATH->include

resource bin/libseasocks.so
resource bin/libseasocks.a
resource src/main/c/**/*.h

config default
  append LIBPATH=@/bin/libseasocks.so
  append LIBPATH=@/bin/libseasocks.a
  append INCLUDEPATH=@/src/main/c//seasocks
end

config ubuntu
  include boost/1.43.ubuntu
  include :default
end

config redhat
  include boost/1.43.redhat5
  include :default
end
